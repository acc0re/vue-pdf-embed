{
  "name": "@acc0re/vue-pdf-embed",
  "version": "2.1.8",
  "description": "PDF embed component for Vue",
  "keywords": [
    "vue",
    "vuejs",
    "pdf"
  ],
  "license": "MIT",
  "author": "Aliaksei Hrynko (https://github.com/hrynko)",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/index.essential.mjs": {
      "import": "./dist/index.essential.mjs",
      "types": "./dist/types/index.essential.d.ts"
    },
    "./dist/styles/*.css": "./dist/styles/*.css"
  },
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "github:acc0re/vue-pdf-embed",
  "scripts": {
    "prepare": "npm run build",
    "dev": "vite -c vite.config.dev.ts",
    "build": "vite build && vite build -c vite.config.essential.ts --emptyOutDir false",
    "postbuild": "vue-tsc -p tsconfig.build.json",
    "test": "vitest",
    "format": "prettier . --write --ignore-path .gitignore"
  },
  "dependencies": {
    "pdfjs-dist": "^4.10.38"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/clean-css": "^4.2.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "clean-css": "^5.3.3",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "vite": "^7.2.2",
    "vitest": "^4.0.10",
    "vue": "^3.5.0",
    "vue-tsc": "^3.1.4"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  }
}
